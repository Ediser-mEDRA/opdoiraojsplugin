msgid "plugins.generic.opdoira.displayName"
msgstr "OP DOI RA Manager Plugin"

msgid "plugins.generic.opdoira.description"
msgstr "Handles depositing and exporting OP DOI RA metadata"

msgid "plugins.generic.opdoira.registrationAgency.name"
msgstr "OP DOI RA"

msgid "plugins.generic.opdoira.deposit.unsuccessful"
msgstr "Registration was not successful! Please check your configuration and try again."

msgid "plugins.importexport.opdoira.displayName"
msgstr "OP DOI RA Export/Registration Plugin"

msgid "plugins.importexport.opdoira.description"
msgstr "Export issue, article and galley metadata in Onix for DOI (O4DOI) format and register DOIs with the OP DOI RA registration agency."

msgid "plugins.importexport.opdoira.settings.description"
msgstr "Please configure the OP DOI RA export plugin before using it for the first time."

msgid "plugins.importexport.opdoira.settings.label"
msgstr "OP DOI RA Settings"

msgid "plugins.importexport.opdoira.registrationAgency.name"
msgstr "OP DOI RA"

msgid "plugins.importexport.opdoira.intro"
msgstr ""
"If you want to register DOIs with OP DOI RA, please follow the directions on the "
"<a href=\"https://ra.publications.europa.eu/doi_howto.htm\" target=\"_blank\">OP DOI RA "
"homepage</a> to receive a username and password.\n"
"If you do not have your own username and password you'll still be able to "
"export into the OP DOI RA XML format (Onix for DOI) but you cannot register your "
"DOIs with OP DOI RA from within OJS. \n"
"Please note that the password will be saved as plain text, i.e. not "
"encrypted, due to the OP DOI RA registration service requirements."

msgid "plugins.importexport.opdoira.settings.form.description"
msgstr "Please configure the OP DOI RA export plugin:"

msgid "plugins.importexport.opdoira.settings.form.username"
msgstr "Username"

msgid "plugins.importexport.opdoira.settings.form.usernameRequired"
msgstr "Please enter your OP DOI RA username. The username may not contain colons."

msgid "plugins.importexport.opdoira.settings.form.registrantName.label"
msgstr "Registrant Name"

msgid "plugins.importexport.opdoira.settings.form.registrantName"
msgstr "The name of the institution registered at OP DOI RA"

msgid "plugins.importexport.opdoira.settings.form.registrantNameRequired"
msgstr "Please enter the institution that is registered at OP DOI RA."

msgid "plugins.importexport.opdoira.settings.form.fromFields"
msgstr "Person to be contacted by OP DOI RA in case of technical inquiries:"

msgid "plugins.importexport.opdoira.settings.form.fromCompany"
msgstr "Institution"

msgid "plugins.importexport.opdoira.settings.form.fromCompanyRequired"
msgstr "Please enter the institution that is technically responsible for DOI export (e.g. the institution hosting your web server)."

msgid "plugins.importexport.opdoira.settings.form.fromName"
msgstr "Contact Person"

msgid "plugins.importexport.opdoira.settings.form.fromNameRequired"
msgstr "Please enter a technical contact person."

msgid "plugins.importexport.opdoira.settings.form.fromEmail"
msgstr "Email"

msgid "plugins.importexport.opdoira.settings.form.fromEmailRequired"
msgstr "Please enter a valid technical contact email."

msgid "plugins.importexport.opdoira.settings.form.publicationCountry"
msgstr "Please choose the country to be informed as 'publication country' to OP DOI RA."

#, fuzzy
msgid "plugins.importexport.opdoira.settings.form.exportIssuesAs"
msgstr ""
"Please choose whether you want to export issues as <a href=\""
"https://ra.publications.europa.eu/metada_specs.htm\" target=\"_blank\">'works' or "
"'manifestations'</a>.<br/>"
"NB: DOIs assigned to articles will be exported to OP DOI RA as <a href=\""
"https://ra.publications.europa.eu/metada_specs.htm\" target=\"_blank\">'works'</a>. "

msgid "plugins.importexport.opdoira.settings.form.work"
msgstr "work"

msgid "plugins.importexport.opdoira.settings.form.manifestation"
msgstr "manifestation"

msgid "plugins.importexport.opdoira.settings.form.exportIssuesAs.label"
msgstr "Issues export as"

msgid "plugins.importexport.opdoira.settings.form.automaticRegistration.description"
msgstr "OJS will register assigned DOIs automatically to OP DOI RA. Please note that this may take a short amount of time after publication to process (e.g. depending on your cronjob configuration). You can check for all unregistered DOIs."

msgid "plugins.importexport.opdoira.settings.form.testMode.description"
msgstr "Use the OP DOI RA test API (testing environment) for DOI registration. Please do not forget to remove this option for the production."

msgid "plugins.importexport.opdoira.workOrProduct"
msgstr ""
"NB: DOIs assigned to articles will be exported to OP DOI RA as <a href=\""
"https://ra.publications.europa.eu/metada_specs.htm\" target=\"_blank\">'works'</a>. "
"DOIs assigned to galleys will be exported as <a href=\"https://ra.publications.europa.eu/"
"metada_specs.htm\" target=\"_blank\">'manifestations'</a>."

msgid "plugins.importexport.opdoira.senderTask.name"
msgstr "OP DOI RA automatic registration task"

msgid "plugins.importexport.opdoira.crossref.label"
msgstr "Deposit to Crossref"

msgid "plugins.importexport.opdoira.crossref"
msgstr "Deposit the DOIs also with the Crossref. (This OP DOI RA service must be enabled for your account.)"

msgid "plugins.importexport.opdoira.crossref.error.cause"
